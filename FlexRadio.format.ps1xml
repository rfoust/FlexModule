                            </ListItem>
                            <ListItem>
                                <PropertyName>Active</PropertyName>
                            </ListItem>
                            <ListItem>
                                <PropertyName>RXAnt</PropertyName>
                            </ListItem>
                            <ListItem>
                                <PropertyName>TXAnt</PropertyName>
                            </ListItem>
                            <ListItem>
                                <PropertyName>Freq</PropertyName>
                            </ListItem>
                        </ListItems>
                    </ListEntry>
                </ListEntries>
            </ListControl>
        </View>
        <View>
            <Name>children</Name>
            <ViewSelectedBy>
                <SelectionSetName>SliceView</SelectionSetName>
            </ViewSelectedBy>
            <GroupBy>
                <PropertyName>Owner</PropertyName>
                <CustomControlName>SliceView-GroupingFormat</CustomControlName>
            </GroupBy>
            <WideControl>
                <WideEntries>
                    <WideEntry>
                        <WideItem>
                            <PropertyName>Index</PropertyName>
                        </WideItem>
                    </WideEntry>
                    <WideEntry>
                        <EntrySelectedBy>
                            <TypeName>Flex.Smoothlake.FlexLib.Slice</TypeName>
                        </EntrySelectedBy>
                        <WideItem>
                            <PropertyName>Index</PropertyName>
                            <FormatString>[{0}]</FormatString>
                        </WideItem>
                    </WideEntry>
                </WideEntries>
            </WideControl>
        </View>

    </ViewDefinitions>
</Configuration>